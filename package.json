{
  "name": "react-native-leaflet-view-2",
  "version": "0.0.4",
  "description": "Forked from 0.2.5 version of @simpiancom/react-native-leaflet-view. A LeafletView component using WebView and Leaflet map for React Native applications",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "cpp",
    "react-native-leaflet.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "assets"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geoinformatica-consulting/react-native-leaflet-view-2.git"
  },
  "author": "(original author) Hung Nguyen <hungnm.k52tnvlkt@gmail.com> (https://github.com/pavel-corsaghin); (secondary author) Kim Sundeen, geoinformatica.consulting@gmail.com, as the secondary author for the forked changes.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geoinformatica-consulting/react-native-leaflet-view-2/issues"
  },
  "homepage": "https://github.com/geoinformatica-consulting/react-native-leaflet-view-2#readme",
  "publishConfig": {
    "access": "public",
    "@geoinformatica_consulting:registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^18.6.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/jest": "^29.5.11",
    "@types/leaflet": "^1.9.8",
    "@types/react": "18.2.0",
    "@types/react-leaflet": "^3.0.0",
    "@types/react-native": "0.72.8",
    "commitlint": "^18.6.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.6",
    "jest": "^29.7.0",
    "leaflet": "^1.9.4",
    "pod-install": "^0.2.0",
    "prettier": "^3.2.4",
    "react": "18.2.0",
    "react-leaflet": "^4.2.1",
    "react-native": "0.74.2",
    "react-native-builder-bob": "^0.23.2",
    "react-native-webview": "^13.6.4",
    "release-it": "^17.0.3",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-native": "0.74.2"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "directories": {
    "example": "example"
  }
}
